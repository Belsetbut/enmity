{
  "exclude": [
    "typings",
    "node_modules"
  ],
  "compilerOptions": {
    "baseUrl": "src",
    "module": "esnext",
    "outDir": "dist",
    "target": "ESNext",
    "jsx": "react",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "noUnusedParameters": false,
    "useDefineForClassFields": false,
    "paths": {
      "@metro": [
        "modules/metro/index"
      ],
      "@metro/*": [
        "modules/metro/*"
      ],
      "@data/*": [
        "data/*"
      ],
      "@utilities": [
        "modules/utilities/index"
      ],
      "@utilities/*": [
        "modules/utilities/*"
      ],
      "@patcher": [
        "modules/patcher/index"
      ],
      "@patcher/*": [
        "modules/patcher/*"
      ],
      "@api/*": [
        "api/*"
      ],
      "@api": [
        "api/index"
      ],
      "@api/settings": [
        "api/settings/index"
      ],
      "@components": [
        "components/index"
      ],
      "@components/*": [
        "components/*"
      ],
      "@managers/*": [
        "managers/*"
      ],
      "@screens/*": [
        "core/screens/*"
      ],
      "@modules/*": [
        "modules/*"
      ],
      "react": [
        "modules/metro/react"
      ],
      "@core": [
        "core/index"
      ],
      "@core/*": [
        "core/*"
      ],
      "react-types": [
        "../node_modules/@types/react"
      ]
    }
  }
}